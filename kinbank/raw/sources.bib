@article{Bowden1983,
 ISSN = {00251496},
 URL = {http://www.jstor.org/stable/2801906},
 abstract = {'Omaha' relationship terminologies, defined by the cross-generational merging of members of MB's patriline, have long been the subject of comparative and theoretical enquiries in anthropology. This article outlines a new approach to the study of 'Omaha' classifications by undertaking an analysis of the structure and social context of a single terminology displaying patrilineal equations. It is argued that cross-generational mergings in the terminology of the Kwoma of Papua New Guinea can best be understood in terms of the structure of the enduring (cross-generational) alliances between groups that arise out marriage. The interpretation developed here differs from existing approaches to Omaha classifications, and from 'alliance' theories generally, principally in that it applies to a society lacking positive marriage rules.},
 author = {Ross Bowden},
 journal = {Man},
 number = {4},
 pages = {745--765},
 publisher = {Royal Anthropological Institute of Great Britain and Ireland},
 title = {Kwoma Terminology and Marriage Alliance: The 'Omaha' Problem Revisited},
 urldate = {2022-09-23},
 volume = {18},
 year = {1983}
}


@article{Hicks1984,
 ISSN = {02579774},
 URL = {http://www.jstor.org/stable/40461873},
 abstract = {The relationship terminology of the Manggarai of western Flores, in Indonesia, has never before been recorded from the region of Boleng. The present analysis identifies it as conforming to a pattern known as an asymmetric alliance system. This characterization matches the institutional arrangements existing between descent groups, which practice affinal contracts with the unilateral cross-cousin on the mother's side. The conclusion of this investigation is that the terminology of the Boleng Manggarai occupies a clear-cut position in what has been proposed as the hypothetical sequence of evolution followed by relationship terminologies on the island of Flores.},
 author = {David Hicks},
 journal = {Anthropos},
 number = {4/6},
 pages = {517--521},
 publisher = {Anthropos Institut},
 title = {A Relationship Terminology of Asymmetric Prescriptive Alliance among the Manggarai of Eastern Indonesia},
 urldate = {2022-09-23},
 volume = {79},
 year = {1984}
}

